<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    activeTab="{Long}0"
    autoScroll="true"
    stateful="false"
    xtype="panel">
    <items jcr:primaryType="cq:WidgetCollection">
        <view
            jcr:primaryType="cq:Widget"
            fieldDescription="View script to render the result item"
            fieldLabel="View"
            name="./viewResource"
            options="/system/list/selectable?category=search-view"
            xtype="selection"/>
        <search-id
            jcr:primaryType="cq:Widget"
            fieldDescription="Identify the list to be listed"
            fieldLabel="Search"
            name="./search-id"
            options="/system/list/selectable?category=search-filter-container"
            xtype="selection"/>
    </items>
</jcr:root>
